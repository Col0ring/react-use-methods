{
  "name": "react-use-methods",
  "version": "2.0.0",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "Col0ring",
    "email": "1561999073@qq.com"
  },
  "keywords": [
    "react",
    "react-hooks"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Col0ring/react-use-methods.git"
  },
  "files": [
    "dist",
    "reducer-mapper",
    "packages",
    "types"
  ],
  "types": "./types",
  "typings": "./types/index.d.ts",
  "main": "./dist/react-use-methods.umd.js",
  "module": "./dist/react-use-methods.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-use-methods.es.js",
      "require": "./dist/react-use-methods.umd.js"
    },
    "./reducer-mapper/*": {
      "import": "./reducer-mapper/*.js",
      "require": "./reducer-mapper/*.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build:plugins": "tsc -p tsconfig-reducer.json && tsc -p tsconfig-reducer.es.json",
    "build:package": "tsc -p tsconfig-package.json && vite build",
    "build": "npm run build:package && npm run build:plugins",
    "postbuild": "npm run lint",
    "serve": "vite preview",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:prettier": "prettier --write '**/*.{ts,tsx,js,json,html,yml,css,less,scss,md}'",
    "lint:eslint": "eslint --fix -c .eslintrc.js --ext .ts,.tsx,.js,.jsx .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "commit": "cz",
    "pre-commit:lint-staged": "lint-staged",
    "commit-msg:commitlint": "commitlint --config commitlint.config.js -e $HUSKY_GIT_PARAMS"
  },
  "dependencies": {
    "immer": "^9.0.6",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-thunk": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.0",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-ali": "^12.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "vite": "^2.5.4",
    "vite-plugin-eslint": "^1.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
